<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

    <Button
        android:id="@+id/button_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Try Catch Kotlin"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_to_try_catch_java"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Try Catch Java"
        app:layout_constraintLeft_toRightOf="@+id/button_first"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="20dp"/>

    <Button
        android:id="@+id/button_to_view_module"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上一页"
        app:layout_constraintLeft_toRightOf="@+id/button_to_try_catch_java"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="20dp"/>

    <Button
        android:id="@+id/showDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示他日志"
        app:layout_constraintTop_toBottomOf="@+id/button_first"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>